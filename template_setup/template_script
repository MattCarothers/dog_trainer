#!/usr/bin/env escript
%% -*- erlang -*-
%%! +sbtu +A1
main(_) ->
  run().

run() ->
    try
        template_setup:main(),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("try~n"),
        io:format("~n")
    catch
        Exception:Reason:Stacktrace ->
            io:format("Exception: ~p, Reason: ~p, Stacktrace: ~p",[Exception,Reason,Stacktrace]),
            io:format("~n"),
            io:format("~n"),
            io:format("~n"),
            io:format("~n"),
            io:format("~n"),
            io:format("~n"),
            io:format("~n"),
            io:format("~n"),
            io:format("~n"),
            io:format("~n"),
            io:format("~n"),
            io:format("~n"),
            io:format("~n"),
            io:format("~n"),
            io:format("~n"),
            io:format("~n"),
            io:format("~n"),
            io:format("~n"),
            io:format("~n"),
            io:format("~n"),
            io:format("~n"),
            io:format("~n"),
            io:format("~n"),
            io:format("~n"),
            io:format("~n"),
            io:format("~n"),
            io:format("~n"),
            io:format("~n"),
            io:format("~n"),
            io:format("~n"),
            io:format("~n"),
            io:format("~n"),
            io:format("~n"),
            io:format("~n"),
            io:format("~n"),
            io:format("~n"),
            io:format("~n"),
            io:format("~n"),
            io:format("~n"),
            io:format("~n"),
            io:format("~n"),
            io:format("~n"),
            io:format("~n"),
            io:format("~n"),
            io:format("~n"),
            io:format("~n"),
            io:format("~n"),
            io:format("~n"),
            io:format("~n"),
            io:format("~n"),
            io:format("~n"),
            io:format("~n"),
            io:format("~n"),
            io:format("~n"),
            io:format("~n"),
            io:format("~n"),
            io:format("catch~n"),
            io:put_chars(<<>>),
            halt(1)
    after
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("~n"),
        io:format("after~n"),
        io:put_chars(<<>>)
    end,
    io:put_chars(<<>>),
    halt(0).
